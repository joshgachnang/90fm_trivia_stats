form(ng-submit="register()")
  md-input-container
    .label Username
    input(ng-model="user.username",required='true')
  md-input-container
    .label Email
    input(ng-model="user.email",type="email",)
  md-input-container
    .label Password
    input(ng-model="user.password",type="password",required='true')
  md-input-container
    .label Phone Number
    input(ng-model="user.phoneNumber",type="phone_number",required='true')
  md-button(type="submit")
